package gameStudio;

import javax.inject.Named;
import javax.persistence.Entity;

@Named
@Entity
public class Score {
	
	private long id;
	
	private String name;
	
	private String game;

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getGame() {
		return game;
	}

	public void setGame(String game) {
		this.game = game;
	}
}
